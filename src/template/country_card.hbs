<div class ="card">
    <div class="card-img">
        <img src="{{flag}}" alt="{{name}}">
    </div>
    <div class="card-body">
    <h2 class="card-title">{{name}}</h2>
    <p class="card-text">Capital:{{capital}}</p>
    <p class ="card-text">Population:{{population}}</p>
    <p class="card-text">Languages:</p>
    <ul class="list-group">
        {{#each languages}}
        <li class="list-item">{{this.name}}</li>
        {{/each}}
    </ul> 
    </div>
</div>